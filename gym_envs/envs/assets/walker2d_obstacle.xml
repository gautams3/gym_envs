      <!-- <key qpos='0 1.22 -0.15708 0.3768 -0.69377 0.141372 -0.5338 0 0.369138'/> -->
      <!-- <key qpos='-0.0138071 1.18886 -0.118397 0.390971 -0.605557 0.0974864 -0.419578 -0.0983474 0.399588'/> -->
      <key qpos='-0.123719 1.19877 -0.11777 0.206598 8.73749e-05 -0.240887 -0.146861 -0.419388 0.448905'/> <!--semi stable stand-->
    </keyframe>
</mujoco>
